<h1>Listing photos</h1>

<table class="table">
  <tr>
    <th>Title</th>
    <th>Site</th>
    <th>User</th>
    <th>Permission</th>
    <th>Attachment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @photos.each do |photo| %>
  <tr>
    <td><%= photo.title %></td>
    <td><%= photo.site %></td>
    <td><%= photo.user %></td>
    <td><%= photo.permission %></td>
    <td><%= photo.attachment %></td>
    <td><%= link_to 'Show', photo %></td>
    <td><%= link_to 'Edit', edit_photo_path(photo) %></td>
    <td><%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Photo', new_photo_path %>
